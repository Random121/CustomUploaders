{
  "Version": "12.4.1",
  "RequestMethod": "POST",
  "RequestURL": "http://imgup.net/upload",
  "Headers": {
    "Accept": "application/json"
  },
  "Body": "MultipartFormData",
  "Arguments": {
    "_method": "put"
  },
  "FileFormName": "image[image][]",
  "URL": "$json:direct_link$",
  "ThumbnailURL": "$json:thumb_link$"
}