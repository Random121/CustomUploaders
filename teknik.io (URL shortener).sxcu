{
  "Version": "12.4.1",
  "Name": "teknik.io (URL shortener)",
  "RequestMethod": "POST",
  "RequestURL": "https://api.teknik.io/v1/Shorten",
  "Headers": {
    "Authorization": "Basic $base64:YOUR_USERNAME_HERE:YOUR_TOKEN_HERE$"
  },
  "Body": "MultipartFormData",
  "Arguments": {
    "url": "$input$",
    "doNotTrack": "true"
  },
  "URL": "$json:result.shortUrl$"
}