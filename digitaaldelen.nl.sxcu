{
  "Version": "12.4.1",
  "DestinationType": "ImageUploader, FileUploader",
  "RequestMethod": "POST",
  "RequestURL": "https://digitaaldelen.nl/index.php",
  "Body": "MultipartFormData",
  "Arguments": {
    "album": "0",
    "agree": "1"
  },
  "FileFormName": "file",
  "RegexList": [
    "1(?<url>.+)"
  ],
  "URL": "$regex:1,url$"
}