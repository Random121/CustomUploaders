{
  "Version": "12.4.1",
  "Name": "teknik.io (Text uploader)",
  "RequestMethod": "POST",
  "RequestURL": "https://api.teknik.io/v1/Paste",
  "Headers": {
    "Authorization": "Basic $base64:YOUR_USERNAME_HERE:YOUR_TOKEN_HERE$"
  },
  "Body": "MultipartFormData",
  "Arguments": {
    "code": "$input$",
    "title": "",
    "syntax": "auto-detect",
    "expireUnit": "never",
    "password": "",
    "hide": "true",
    "doNotTrack": "true"
  },
  "URL": "$json:result.url$"
}